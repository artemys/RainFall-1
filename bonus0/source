void
p (char *buffer, const char *s) {

    puts(s);
    char buff[4104];
    read(0, buff, 4096);
    const char *b = strchr(buff, '\n');
    *b = '\0';
    strncpy(buffer, buff, 20);
}

void
pp (char *buffer) {

    p(&buffer[0], "-");
    p(&buffer[28], "-");
}

int
main (void) {

    char buffer[64];

    pp(buffer);
    strcat();
    puts(buffer);
}
